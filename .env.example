# PA eDocket Desktop - Production Environment Configuration

# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/pa_edocket_production

# Vector Search Configuration
QDRANT_URL=http://localhost:6333
QDRANT_API_KEY=your_qdrant_api_key_here

# Full-Text Search Configuration
OPENSEARCH_URL=https://localhost:9200
OPENSEARCH_USERNAME=admin
OPENSEARCH_PASSWORD=admin

# API Keys for Data Sources
COURTLISTENER_API_TOKEN=your_courtlistener_token_here
GOVINFO_API_KEY=your_govinfo_api_key_here

# Application Configuration
NODE_ENV=production
API_BASE_URL=http://localhost:3000
VITE_API_BASE=http://localhost:3000

# Security Configuration
JWT_SECRET=your_jwt_secret_here
ENCRYPTION_KEY=your_32_character_encryption_key_here

# File Storage
DATA_DIR=./data
UPLOAD_DIR=./uploads
TEMP_DIR=./temp

# Rate Limiting
COURTLISTENER_RATE_LIMIT=5
GOVINFO_RATE_LIMIT=10
DEFAULT_RATE_LIMIT=30

# Logging
LOG_LEVEL=info
LOG_FORMAT=json

# Worker Configuration
WORKER_CONCURRENCY=4
BATCH_SIZE=100
RETRY_ATTEMPTS=3

# E-Filing Configuration
PACFILE_BASE_URL=https://www.pacourts.us/pacfile
PACFILE_TIMEOUT=30000

# Monitoring
METRICS_PORT=9090
HEALTH_CHECK_PORT=8080

# Development/Testing
SKIP_SSL_VERIFY=false
DEBUG_MODE=false
TEST_MODE=false
