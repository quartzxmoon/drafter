{
  "name": "pa-edocket-desktop",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "description": "PA eDocket Desktop - Production-grade cross-platform desktop application for Pennsylvania court docket management",
  "author": "PA eDocket Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/quartzxmoon/drafter.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "format": "prettier --write src/**/*.{ts,tsx,js,jsx,json,css,md}",
    "test": "echo \"Error: no test specified\" && exit 1",
    "env:check": "node scripts/env-check.js",
    "db:migrate": "node scripts/migrate.js",
    "db:seed": "node scripts/seed.js",
    "ingest:courtlistener:backfill": "node scripts/ingest-courtlistener.js --backfill",
    "ingest:courtlistener:since": "node scripts/ingest-courtlistener.js --since",
    "ingest:govinfo:backfill": "node scripts/ingest-govinfo.js --backfill",
    "ingest:govinfo:since": "node scripts/ingest-govinfo.js --since",
    "ingest:resume": "node scripts/ingest-resume.js",
    "ingest:verify": "node scripts/ingest-verify.js",
    "draft:run": "node scripts/draft-runner.js",
    "search:create-indexes": "node scripts/create-search-indexes.js",
    "worker:start": "node scripts/worker.js",
    "api:start": "node scripts/api-server.js"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@tailwindcss/postcss": "^4.1.14",
    "@tanstack/react-table": "^8.21.3",
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-dialog": "^2.4.0",
    "@tauri-apps/plugin-http": "^2.5.2",
    "@tauri-apps/plugin-notification": "^2.3.1",
    "@tauri-apps/plugin-opener": "^2",
    "@tauri-apps/plugin-os": "^2.3.1",
    "@tauri-apps/plugin-shell": "^2.3.1",
    "@tauri-apps/plugin-sql": "^2.3.0",
    "@tauri-apps/plugin-updater": "^2.9.0",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "cors": "^2.8.5",
    "helmet": "^8.0.0",
    "compression": "^1.7.5",
    "pg": "^8.13.1",
    "node-fetch": "^3.3.2",
    "form-data": "^4.0.1",
    "pdf-parse": "^1.1.1",
    "pdf2pic": "^3.1.3",
    "sharp": "^0.33.5",
    "archiver": "^7.0.1",
    "json2csv": "^5.0.7",
    "lucide-react": "^0.545.0",
    "pdf-lib": "^1.17.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.65.0",
    "react-router-dom": "^7.9.4",
    "zod": "^3.23.8",
    "zustand": "^5.0.8",
    "winston": "^3.17.0",
    "prom-client": "^14.2.0",
    "cron": "^3.2.0",
    "bull": "^4.12.9",
    "ioredis": "^5.4.2"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2",
    "@types/node": "^24.7.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.37.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.14",
    "typescript": "~5.8.3",
    "vite": "^7.0.4"
  }
}
